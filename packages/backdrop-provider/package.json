{
  "name": "@mgcrea/react-native-backdrop-provider",
  "version": "0.9.0",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:source && yarn build:typings",
    "build:source": "NODE_ENV=production babel --verbose --source-maps --extensions .js,.ts,.tsx --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "NODE_ENV=production tsc --declaration",
    "typecheck": "tsc --pretty --noEmit",
    "test": "CI=true jest --runInBand --colors"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-native": ">=0.59"
  },
  "devDependencies": {
    "@mgcrea/react-native-button": "0.9.0",
    "@babel/cli": "^7.13.10",
    "@storybook/react-native": "^5.3.25",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.63.51",
    "@types/react-test-renderer": "^17.0.1",
    "react": "^17.0.1",
    "react-native": "^0.63.4",
    "react-native-testing-library": "^2.2.0",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.2.3"
  }
}
